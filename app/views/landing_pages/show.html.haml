.page-header
  %h1
    Landing Pages
    %small Subtext for header
.panel.panel-default
  / Default panel contents
  .panel-heading
    = @landing_page.name
    %button.btn.btn-default.btn-sm.pull-right{"data-target" => "#scriptModal", "data-toggle" => "modal"} Code Snippet
    = link_to 'Edit', edit_landing_page_path(@landing_page), class: "btn btn-default btn-sm pull-right"
  .panel-body
    %p= @landing_page.url
  %table.table
    %tr
      %th Date
      %th Form
    %tr
      %td January
      %td $100

= render 'script'